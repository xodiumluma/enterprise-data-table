{
  "name": "@ag-grid-community/core",
  "version": "31.0.0",
  "description": "Advanced Data Grid / Data Table supporting Javascript / Typescript / React / Angular / Vue",
  "main": "./dist/esm/es6/main.mjs",
  "module": "./dist/esm/es6/main.mjs",
  "types": "./dist/esm/es6/main.d.ts",
  "scripts": {
    "lint": "npx tslint -p tsconfig.cjs.es5.json src/ts/**/*.ts",
    "lint-fix": "npx tslint -p tsconfig.cjs.es5.json -c tslint-fix.json --fix src/ts/**/*.ts",
    "precheck": "npx tsc -p tsconfig-precheck.json",
    "build": "rimraf dist/tsconfig.cjs.es5.docs.tsbuildinfo && npm run precheck && npx gulp && npx tsc -p tsconfig.typings.json",
    "build-prod": "npm run precheck && npx gulp prod && npx tsc -p tsconfig.typings.json --sourceMap false",
    "test": "npx jest && npm run check-grid-options && npm run check-grid-api-exports",
    "package": "node ../../module-build/rollup/build.js",
    "clean": "npx rimraf dist typings",
    "check-grid-options": "npx ts-node -P tsconfig.cjs.es5.json scripts/check-gridOptions-propertyKeys.ts",
    "check-grid-api-exports": "node scripts/check-gridApi-interfaces-exported.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "keywords": [
    "ag",
    "ag-grid",
    "datagrid",
    "data-grid",
    "datatable",
    "data-table",
    "grid",
    "table",
    "react",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "reactjs",
    "vue",
    "vuejs"
  ],
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie >= 0",
    "not ie_mob >= 0"
  ],
  "homepage": "https://www.ag-grid.com/",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@babel/types": "7.12.7",
    "@types/node": "12.20.20",
    "cache-loader": "^4.1.0",
    "del": "^5.1.0",
    "eslint": "^6.3.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-ie11": "^1.0.0",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-flatmap": "^1.0.0",
    "gulp-header": "^2.0.0",
    "gulp-if": "3.0.0",
    "gulp-jasmine": "^4.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-stylus": "^2.7.0",
    "gulp-tsd": "^0.1.0",
    "gulp-typescript": "^5.0.1",
    "gulp-uglify": "^3.0.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "json-stable-stringify": "^1.0.0",
    "merge2": "^1.2.0",
    "nib": "^1.1.0",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.0",
    "ts-morph": "^14.0.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.19.0",
    "typescript": "~4.7.4",
    "url-loader": "^2.1.0",
    "vinyl-buffer": "^1.0.0",
    "webpack": "4.46.0"
  },
  "publishConfig": {
    "access": "public"
  }
}