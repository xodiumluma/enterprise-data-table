{
  "name": "@ag-grid-enterprise/all-modules",
  "version": "29.3.0",
  "description": "Advanced Data Grid / Data Table supporting Javascript / Typescript / React / Angular / Vue",
  "main": "./dist/cjs/es5/main.js",
  "module": "./dist/esm/es5/main.js",
  "types": "./dist/cjs/es5/main.d.ts",
  "private": true,
  "scripts": {
    "clean": "rimraf dist ts-lint-rules/dist .hash",
    "build-cjs": "rimraf dist/tsconfig.cjs.es5.docs.tsbuildinfo && npx tsc -p tsconfig.cjs.es5.json && npx tsc -p tsconfig.cjs.es6.json",
    "build-esm": "npx tsc -p tsconfig.esm.es5.json && npx tsc -p tsconfig.esm.es6.json",
    "build-css": "npx gulp copy-grid-core-styles",
    "build-cjs-prod": "npx tsc -p tsconfig.cjs.es5.json --sourceMap false && npx tsc -p tsconfig.cjs.es6.json --sourceMap false",
    "build-esm-prod": "npx tsc -p tsconfig.esm.es5.json --sourceMap false && npx tsc -p tsconfig.esm.es6.json --sourceMap false",
    "build": "npm run precheck && npm run lint && npm run build-cjs && npm run build-esm && npx gulp copy-grid-core-styles && npm run hash",
    "build-prod": "npm run precheck && npm run lint && npm run build-cjs-prod && npm run build-esm-prod && npx gulp copy-grid-core-styles && npm run hash",
    "hash": "sh ../../scripts/hashDirectory.sh > .hash",
    "package": "npm run webpack-updater && npx gulp webpack-all-no-clean && npm run rollup",
    "watchWebpack": "npx webpack --watch",
    "precheck": "npx tsc -p tsconfig-precheck.json",
    "webpack-updater": "node webpackUpdater.js",
    "rollup": "node ../../module-build/rollup/build.js --bundlePrefix ag-grid-enterprise --esmType autoRegister",
    "lint": "npx tsc -p tsconfig-lint.json && npx tslint --project tsconfig.cjs.es5.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "keywords": [
    "enterprise",
    "ag",
    "ag-grid",
    "datagrid",
    "data-grid",
    "datatable",
    "data-table",
    "grid",
    "table",
    "react",
    "table",
    "angular",
    "angular-component",
    "react",
    "react-component",
    "reactjs",
    "vue",
    "vuejs"
  ],
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "Commercial",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie >= 0",
    "not ie_mob >= 0",
    "not blackberry > 0"
  ],
  "homepage": "https://www.ag-grid.com/",
  "dependencies": {
    "@ag-grid-community/all-modules": "~29.3.0",
    "@ag-grid-enterprise/charts": "~29.3.0",
    "@ag-grid-enterprise/clipboard": "~29.3.0",
    "@ag-grid-enterprise/column-tool-panel": "~29.3.0",
    "@ag-grid-enterprise/core": "~29.3.0",
    "@ag-grid-enterprise/excel-export": "~29.3.0",
    "@ag-grid-enterprise/filter-tool-panel": "~29.3.0",
    "@ag-grid-enterprise/master-detail": "~29.3.0",
    "@ag-grid-enterprise/menu": "~29.3.0",
    "@ag-grid-enterprise/multi-filter": "~29.3.0",
    "@ag-grid-enterprise/range-selection": "~29.3.0",
    "@ag-grid-enterprise/rich-select": "~29.3.0",
    "@ag-grid-enterprise/row-grouping": "~29.3.0",
    "@ag-grid-enterprise/server-side-row-model": "~29.3.0",
    "@ag-grid-enterprise/set-filter": "~29.3.0",
    "@ag-grid-enterprise/side-bar": "~29.3.0",
    "@ag-grid-enterprise/sparklines": "~29.3.0",
    "@ag-grid-enterprise/status-bar": "~29.3.0",
    "@ag-grid-enterprise/viewport-row-model": "~29.3.0",
    "@ag-grid-community/styles": "~29.3.0"
  },
  "devDependencies": {
    "@babel/types": "7.12.7",
    "@ag-grid-community/core": "~29.3.0",
    "@types/node": "12.20.20",
    "css-loader": "3.2.0",
    "cssnano": "4.1.0",
    "eslint": "6.3.0",
    "eslint-plugin-compat": "3.3.0",
    "eslint-plugin-ie11": "1.0.0",
    "glob": "7.1.4",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-cssnano": "2.1.3",
    "gulp-header": "2.0.9",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-tslint": "8.1.0",
    "gulp-typescript": "5.0.1",
    "http-server": "0.11.1",
    "merge2": "1.2.0",
    "postcss-loader": "3.0.0",
    "rimraf": "3.0.2",
    "rollup": "1.21.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "style-loader": "1.0.0",
    "string-replace-loader": "3.1.0",
    "terser-webpack-plugin": "2.0.1",
    "tslint": "5.19.0",
    "typescript": "~4.3.5",
    "webpack": "4.39.0",
    "webpack-stream": "5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}