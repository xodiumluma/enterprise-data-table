{
  "name": "ag-grid-vue",
  "description": "AG Grid Vue 2 Component",
  "version": "31.0.0",
  "author": "Sean Landsman <sean@thelandsmans.com>",
  "license": "MIT",
  "main": "main.js",
  "typings": "main.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid.git"
  },
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid/issues"
  },
  "homepage": "https://www.ag-grid.com/",
  "keywords": [
    "grid",
    "data",
    "table",
    "vue",
    "vuejs"
  ],
  "scripts": {
    "clean": "rimraf dist lib main.d.ts main.js",
    "bundle": "NODE_OPTIONS=--openssl-legacy-provider npx cross-env vue-cli-service build --target lib src/AgGridVue.ts && rm ./dist/demo.html",
    "npm": "npx tsc -p tsconfig-lib.json",
    "build": "echo \"module.exports = require('./build')\" > ./node_modules/vue-template-compiler/index.js && npx gulp copy-from-module-source && npm run npm && npm run bundle",
    "build-docs-initial": "npm run build",
    "build-prod": "npm run build",
    "lint": "npx cross-env vue-cli-service lint"
  },
  "devDependencies": {
    "@types/node": "12.12.22",
    "@vue/cli-plugin-typescript": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "ag-grid-community": "~31.0.0",
    "@ag-grid-community/vue": "~31.0.0",
    "gulp": "^4.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-typescript": "^5.0.0",
    "merge-stream": "^2.0.0",
    "typescript": "~4.7.4",
    "vue": "2.6.10",
    "vue-class-component": "^7.0.0",
    "vue-property-decorator": "^8.2.0",
    "vue-template-compiler": "^2.6.10",
    "rimraf": "3.0.2",
    "cross-env": "7.0.3"
  },
  "dependencies": {
    "ag-grid-community": "~31.0.0"
  },
  "peerDependencies": {
    "vue": ">= 2.2 <= 2.5.17 || >= 2.5.20",
    "vue-property-decorator": "^7.2.0 || ^8.0.0 || ^9.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}